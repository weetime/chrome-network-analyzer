{
  "env": {
    "browser": true,
    "es2021": true,
    "webextensions": true
  },
  "extends": ["eslint:recommended", "prettier"],
  "parserOptions": {
    "ecmaVersion": 2022,
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": "off",
    "no-console": "off",
    "no-undef": ["error", { "typeof": true }],
    "no-prototype-builtins": "warn"
  },
  "globals": {
    "TableManager": "readonly",
    "I18n": "readonly",
    "showRequestDetails": "readonly",
    "applyFilters": "readonly",
    "requestNetworkData": "readonly"
  }
}
